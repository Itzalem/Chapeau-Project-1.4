@model IEnumerable<RunningOrderVm>
<h2>Running Orders</h2>
<button onclick="location.reload()">Refresh</button>
<table>
    <thead>
        <tr><th>Table</th><th>Ordered At</th><th>Waiting (min)</th></tr>
    </thead>
    <tbody>
        @foreach (var order in Model)
        {
            <tr>
                <td>@order.TableNumber</td>
                <td>@order.OrderTime:HH:mm</td>
                <td>@order.WaitingTime</td>
            </tr>
        }
    </tbody>
</table>

*@

